{
  "$schema": "https://railway.com/railway.schema.json",
  "deploy": {
    "runtime": "V2",
    "startCommand": "pnpm --filter @hey/rpc start",
    "healthcheckPath": "/",
    "healthcheckTimeout": 60,
    "restartPolicyType": "ALWAYS",
    "sleepApplication": false,
    "limitOverride": {
      "containers": {
        "cpu": 5,
        "memoryBytes": 5000000000
      }
    }
  }
}
